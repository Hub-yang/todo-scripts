{
  "name": "@huberyyang/todo-scripts",
  "type": "module",
  "version": "1.0.9",
  "packageManager": "pnpm@10.29.2",
  "description": "Some public executable scripts",
  "author": "HuberyYang",
  "license": "MIT",
  "homepage": "https://github.com/Hub-yang/todo-scripts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hub-yang/todo-scripts.git"
  },
  "bugs": {
    "url": "https://github.com/Hub-yang/todo-scripts/issues"
  },
  "keywords": [
    "scripts",
    "node",
    "bash",
    "bin",
    "sh"
  ],
  "bin": {
    "hubery": "bin/index.js"
  },
  "files": [
    "bin",
    "dist"
  ],
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "tsdown --watch",
    "preview": "nr build && npx hubery commitlint-init",
    "build": "tsdown",
    "build:prod": "nr typecheck && nr lint:fix && tsdown",
    "test": "vitest",
    "typecheck": "tsc",
    "release": "nr build && bumpp && npm publish",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "taze": "npx taze major -r",
    "prepare": "husky",
    "commitlint": "commitlint --edit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^7.4.2",
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@huberyyang/todo-scripts": "^1.0.9",
    "@types/node": "^20.19.0 || >=22.12.0",
    "bumpp": "^10.4.1",
    "eslint": "^10.0.0",
    "execa": "^9.6.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "mri": "^1.2.0",
    "picocolors": "^1.1.1",
    "terminal-link": "^5.0.0",
    "tsdown": "^0.20.3",
    "vitest": "^4.0.18",
    "yocto-spinner": "^1.1.0"
  },
  "lint-staged": {
    "*": "eslint . --fix"
  }
}
